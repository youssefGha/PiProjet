{% trans_default_domain 'FOSUserBundle' %}
{% extends 'base.html.twig' %}
{% block content %}
<div style="margin-top: 200px;margin-bottom: 668px;margin-left: 800px">
{{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register_fournisseur'), 'attr': {'class': 'fos_user_registration_register'}}) }}
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" />
