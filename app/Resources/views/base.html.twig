<!DOCTYPE html>
<html style="background: #FFFFFF">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href={{  asset('front/styles/bootstrap4/bootstrap.min.css')}}>
            <link rel="stylesheet" type="text/css" href={{ asset('front/plugins/font-awesome-4.7.0/css/font-awesome.min.css') }}  >
            <link rel="stylesheet" type="text/css" href={{ asset('front/plugins/OwlCarousel2-2.2.1/owl.carousel.css') }}>
            <link rel="stylesheet" type="text/css" href={{ asset('front/plugins/OwlCarousel2-2.2.1/owl.theme.default.css') }}>
            <link rel="stylesheet" type="text/css" href={{ asset('front/plugins/OwlCarousel2-2.2.1/animate.css') }}>
            <link rel="stylesheet" type="text/css" href={{ asset('front/styles/main_styles.css') }}>
            <link rel="stylesheet" type="text/css" href={{ asset('front/styles/responsive.css') }}>
            <link href="{{ asset('back/lib/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet">
            <!--external css-->
            <link href="{{ asset('back/lib/font-awesome/css/font-awesome.css')}}" rel="stylesheet" />
            <link rel="stylesheet" type="text/css" href="{{ asset('back/css/zabuto_calendar.css')}}">
            <link rel="stylesheet" type="text/css" href="{{ asset('lib/gritter/css/jquery.gritter.css') }}" />
            <!-- Custom styles for this template -->
            <link href="{{ asset('back/css/style.css')}}" rel="stylesheet">
            <link href="{{ asset('back/css/style-responsive.css')}}" rel="stylesheet">
        {% endblock %}
        <link href="{{ asset('back/img/favicon.png')}}" rel="icon">
        <link href="{{ asset('back/img/apple-touch-icon.png') }}" rel="apple-touch-icon">
    </head>
    <body style="background: #FFFFFF">
        {% block header %}
            <header class="header">
                <div class="header_container">
                    <div class="container">
                        <div class="row">
                            <div class="col">
                                <div class="header_content d-flex flex-row align-items-center justify-content-start">
                                    <div class="logo"><a href="#">Sublime.</a></div>
                                    <nav class="main_nav">
                                        <ul>
                                            <li class="hassubs active">
                                                <a href="#">Home</a>
                                                <ul>
                                                    <li><a href="#">Categories</a></li>
                                                    <li><a href="{{ path('produit_select') }}">Produits</a></li>
                                                    <li><a href="{{ path('commande_select') }}">Commandes</a></li>
                                                    <li><a href="{{ path('vente_all') }}">Reclamations</a></li>
                                                    <li><a href="{{ path('panier_select') }}">Panier</a></li>

                                                </ul>
                                            </li>
                                            <li class="hassubs">
                                                <a href="#">Categories</a>
                                                <ul>
                                                    <li><a href="#">Jeans</a></li>
                                                    <li><a href="#">Pulls</a></li>
                                                    <li><a href="#">Vestes</a></li>
                                                    <li><a href="#">Shorts</a></li>
                                                    <li><a href="#">Baskets</a></li>
                                                    <li><a href="#">Chesmises</a></li>
                                                </ul>
                                            </li>
                                            <li><a href="{{ path('produit_select') }}">Produits</a></li>
                                            <li><a href="{{ path('appel_offre') }}">Appel d'offre</a></li>
                                            {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                                                <li><a href="{{ path('panier_select') }}">Panier</a></li>
                                                <li><a href="{{ path('commande_select') }}">Commandes</a></li>
                                                <li><a href="{{ path('vente_all') }}">Reclamations</a></li>
                                                <li><a href="{{ path('fos_user_security_logout') }}">Se d√©connecter</a></li>
                                            {% else %}
                                                <li><a class="logout" href="{{ path('fos_user_registration_register_client') }}">S'inscrire</a></li>
                                                <li><a href="{{ path('fos_user_security_login') }}">Se connecter</a></li>
                                            {% endif %}
                                        </ul>

                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        {% endblock %}
        {% block content %}

        {% endblock %}
        {% block footer %}
            <footer class="site-footer" style="position:absolute; bottom: -200%; left: 0; width: 100%;">
                <div class="text-center">
                    <div class="footer_logo"><a href="index.html">Sublime.</a></div>
                    <div class="copyright ml-auto mr-auto">
                        Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | made with <i class="fa fa-heart-o" aria-hidden="true"></i> by
                        Outsiders</div>
                    <a href="index.html#" class="go-top">
                        <i class="fa fa-angle-up"></i>
                    </a>
                </div>
            </footer>
        {% endblock %}
        {% block javascripts %}
            <script src={{ asset('front/js/jquery-3.2.1.min.js') }}></script>
            <script src={{ asset('front/styles/bootstrap4/popper.js') }}></script>
            <script src={{ asset('front/styles/bootstrap4/bootstrap.min.js') }}></script>
            <script src={{ asset('front/plugins/greensock/TweenMax.min.js') }}></script>
            <script src={{ asset('front/plugins/greensock/TimelineMax.min.js') }}></script>
            <script src={{ asset('front/plugins/scrollmagic/ScrollMagic.min.js') }}></script>
            <script src={{ asset('front/plugins/greensock/animation.gsap.min.js') }}></script>
            <script src={{ asset('front/plugins/greensock/ScrollToPlugin.min.js') }}></script>
            <script src={{ asset('front/plugins/OwlCarousel2-2.2.1/owl.carousel.js') }}></script>
            <script src={{ asset('front/plugins/Isotope/isotope.pkgd.min.js') }}></script>
            <script src={{ asset('front/plugins/easing/easing.js') }}></script>
            <script src={{ asset('front/plugins/parallax-js-master/parallax.min.js') }}></script>
            <script src={{ asset('front/js/custom.js') }}></script>
            <script src="{{ asset('back/lib/bootstrap/js/bootstrap.min.js') }}"></script>
            <script class="include" type="text/javascript" src="{{ asset('back/lib/jquery.dcjqaccordion.2.7.js') }}"></script>
            <script src="{{ asset('back/lib/jquery.scrollTo.min.js') }}"></script>
            <script src="{{ asset('back/lib/jquery.nicescroll.js') }}" type="text/javascript"></script>
            <script src="{{ asset('back/lib/jquery.sparkline.js') }}"></script>
            <script src="{{ asset('back/lib/common-scripts.js') }}"></script>
            <script type="text/javascript" src="{{ asset('back/lib/gritter/js/jquery.gritter.js') }}"></script>
            <script type="text/javascript" src="{{ asset('back/lib/gritter-conf.js') }}"></script>
            <!--script for this page-->
            <script src="{{ asset('back/lib/sparkline-chart.js') }}"></script>
            <script src="{{ asset('back/lib/zabuto_calendar.js') }}"></script>
        {% endblock %}
    </body>
</html>
